{
    "id": "c1d0df7f",
    "name": "Erase.bg",
    "version": "1.0.0",
    "main": "index.html",
    "manifestVersion": 5,
    "host": [
        {
            "app": "PS",
            "minVersion": "25.1.0"
        }
    ],
    "requiredPermissions": {
        "allowCodeGenerationFromStrings": true,
        "launchProcess": {
            "schemes": ["http", "https"],
            "extensions": [".svg", ".png"]
        },
        "webview": {
            "allow": "yes",
            "domains": ["https://*.adobe.com", "https://*.google.com"]
        },
        "network": {
            "domains": [
                "https://console.pixelbin.io",
                "https://api.pixelbin.io",
                "https://cdn.pixelbin.io",
                "https://www.pixelbin.io",
                "https://www.erase.bg"
            ]
        },
        "ipc": {
            "enablePluginCommunication": true
        }
    },
    "entrypoints": [
        {
            "type": "command",
            "id": "goToDashboard",
            "label": { "default": "Go to dashboard" }
        },
        {
            "type": "command",
            "id": "buyCredits",
            "label": { "default": "Buy credits" }
        },
        {
            "type": "command",
            "id": "howItWorks",
            "label": { "default": "How it works" }
        },
        {
            "type": "panel",
            "id": "app",
            "label": { "default": "Remove Background (Erase.bg)" },
            "minimumSize": { "width": 260, "height": 540 },
            "maximumSize": { "width": 260, "height": 540 },
            "preferredDockedSize": { "width": 260, "height": 540 },
            "preferredFloatingSize": { "width": 260, "height": 540 },
            "icons": [
                {
                    "width": 23,
                    "height": 23,
                    "path": "icons/erasebg.png",
                    "scale": [1, 2],
                    "theme": ["all"],
                    "species": ["generic"]
                }
            ]
        }
    ],
    "icons": [
        {
            "width": 23,
            "height": 23,
            "path": "icons/erasebg.png",
            "scale": [1, 2],
            "theme": ["all"],
            "species": ["generic"]
        }
    ]
}
